*nat
<% if @dnsserver %>
-A PREROUTING -i eth0 -p udp --dport 53 -j DNAT --to 10.0.0.4:3053
-A PREROUTING -i eth0 -p tcp --dport 53 -j DNAT --to 10.0.0.4:3053
<% end %>
<% if @webserver %>
-A PREROUTING -i eth0 -p tcp --dport 80 -j DNAT --to 10.0.0.4:3080
<% end %>
<% if @ssl %>
-A PREROUTING -i eth0 -p tcp --dport 443 -j DNAT --to 10.0.0.4:3443
<% end %>
