[PlexConnect]
certfile = <%= @certfile %>
enable_dnsserver = <%= @enable_dnsserver ? 'True' : 'False' %>
enable_plexconnect_autodetect = True
enable_plexgdm = False
enable_webserver_ssl = True
ip_dnsmaster = 8.8.8.8
ip_plexconnect = 0.0.0.0
ip_pms = <%= @ip_pms %>
ip_webserver = 0.0.0.0
loglevel = Normal
logpath = <%= @logpath %>
port_dnsserver = <%= @port_dnsserver %>
port_pms = <%= @port_pms %>
port_ssl = <%= @port_ssl %>
port_webserver = <%= @port_webserver %>
prevent_atv_update = True
hosttointercept = <%= @hosttointercept %>
